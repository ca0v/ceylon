{
  "name": "ceylon",
  "version": "0.0.13",
  "description": "An assertion library for Unit Testing written in Typescript",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "author": {
    "name": "Dylan Parry",
    "email": "me@dylanparry.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanparry/ceylon"
  },
  "bugs": {
    "url": "https://github.com/dylanparry/ceylon/issues"
  },
  "tags": [
    "unit test",
    "test",
    "typescript",
    "assert",
    "assertion",
    "expect",
    "spec"
  ],
  "devDependencies": {
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3",
    "typings": "^1.3.3"
  },
  "scripts": {
    "pretest": "rimraf ./temp && typings install && tsc --target ES5 --outDir ./temp",
    "test": "mocha ./temp/tests/*.spec.js ./temp/tests/**/*.spec.js",
    "posttest": "rimraf ./temp",
    "prepublish": "rimraf ./lib && typings install && tsc --target ES5 --outDir ./lib --declaration ./src/index.ts ./typings/index.d.ts"
  },
  "engines": {
    "node": ">=0.10.46",
    "iojs": "3.3.1"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
